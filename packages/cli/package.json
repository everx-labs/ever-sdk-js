{
  "name": "@eversdk/cli",
  "version": "1.46.1",
  "description": "Evercloud CLI",
  "homepage": "https://everx.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonlabs/ever-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/tonlabs/ever-sdk-js/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "compile": "npm test -- c",
    "compile-kamikadze": "npm run compile -- ./contracts/Kamikadze.sol",
    "compile-touch": "npm run compile -- ./contracts/Touch.sol",
    "clean": "rimraf *.js *.tgz",
    "eslint": "eslint '**/*.ts'",
    "prepublishOnly": "NODE_ENV=development npm i",
    "prepack": "NODE_ENV=production npm run build",
    "prettier": "prettier --config .prettierrc '**/*.ts' --write",
    "test": "npm run clean; npx ts-node index.ts"
  },
  "bin": {
    "evercloud": "./cli.js"
  },
  "files": [
    "index.js",
    "cli.js",
    "compile.js",
    "giver.js",
    "graphql.js",
    "kamikadze.js",
    "touch.js",
    "utils.js",
    "contracts/Kamikadze.js",
    "contracts/Kamikadze.sol",
    "contracts/Touch.js",
    "contracts/Touch.sol"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "TON Labs LTD.",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "@eversdk/appkit": "^0.3.8",
    "@eversdk/core": "^1.46.1",
    "@eversdk/lib-node": "^1.46.1",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "everdev": "^1.7.0",
    "qrcode": "^1.5.3",
    "tvm-solidity-parser": "^0.14.75"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "18",
    "npm": "9"
  }
}
